{
  "cli": {
    "version": ">= 7.3.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "autoIncrement": true
    },
    "preview": {
      "distribution": "store",
      "autoIncrement": true
    },
    "production": {}
  },
  "submit": {
    "production": {
      "android": {
        "track": "production",
        "releaseStatus": "completed",
        "serviceAccountKeyPath": "../tgc-service-account.json"
      }
    },
    "preview": {
      "android": {
        "track": "alpha",
        "releaseStatus": "draft",
        "serviceAccountKeyPath": "../tgc-service-account.json"
      }
    }
  }
}
